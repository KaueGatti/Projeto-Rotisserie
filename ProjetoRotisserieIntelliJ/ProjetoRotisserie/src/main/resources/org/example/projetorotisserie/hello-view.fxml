<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="331.0" prefWidth="455.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.12" fx:controller="org.example.projetorotisserie.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <AnchorPane>
      <children>
         <DatePicker fx:id="data" layoutX="120.0" prefHeight="33.6" prefWidth="415.2" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      </children>
   </AnchorPane>

  <Label fx:id="welcomeText" />
   <Button fx:id="button" mnemonicParsing="false" onAction="#onHelloButtonClick" prefHeight="26.0" prefWidth="99.0" text="Button" />
</VBox>
