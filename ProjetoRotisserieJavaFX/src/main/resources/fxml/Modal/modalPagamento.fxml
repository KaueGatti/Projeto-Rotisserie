    <AnchorPane prefHeight="166.0" prefWidth="329.0">
      <children>
         <Label alignment="CENTER" layoutX="1.0" layoutY="14.0" prefHeight="18.0" prefWidth="328.0" text="Qual a forma de pagamento?">
            <font>
               <Font size="20.0" />
            </font>
         </Label>
         <Button fx:id="btnImprimir" layoutX="23.0" layoutY="121.0" mnemonicParsing="false" onAction="#Imprimir" prefHeight="30.0" prefWidth="132.0" text="Imprimir" />
         <Button fx:id="btnCancelar" layoutX="174.0" layoutY="121.0" mnemonicParsing="false" onAction="#Cancelar" prefHeight="30.0" prefWidth="132.0" text="Cancelar" />
         <ComboBox fx:id="comboBoxPagamento" layoutX="59.0" layoutY="70.0" prefHeight="26.0" prefWidth="212.0" />
      </children></AnchorPane>
</Scene>
