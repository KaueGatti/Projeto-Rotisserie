<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<Scene fx:id="scene" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="my.company.projetorotisseriejavafx.Controller.ModalDetalhesProdutoController">
    <AnchorPane prefHeight="300.0" prefWidth="480.0">
      <children>
         <Label alignment="CENTER" prefHeight="60.0" prefWidth="479.0" text="Detalhes do Produto">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
         <Label layoutX="131.0" layoutY="60.0" text="Produto">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="TFProduto" editable="false" layoutX="125.0" layoutY="86.0" prefHeight="26.0" prefWidth="229.0" />
         <Label layoutX="131.0" layoutY="112.0" text="Quantidade">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="TFQuantidade" editable="false" layoutX="125.0" layoutY="138.0" prefHeight="26.0" prefWidth="229.0" />
         <Label layoutX="131.0" layoutY="164.0" text="Subtotal">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <TextField fx:id="TFSubtotal" editable="false" layoutX="125.0" layoutY="190.0" prefHeight="26.0" prefWidth="229.0" />
         <Button fx:id="btnFechar" layoutX="125.0" layoutY="237.0" mnemonicParsing="false" onAction="#fechar" prefHeight="32.0" prefWidth="224.0" text="Fechar">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
      </children></AnchorPane>
</Scene>
